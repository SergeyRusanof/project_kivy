<ItemButton@Button>:
    background_color: [.20, .35, .25, 1]
<ItemBox@BoxLayout>:
    orientation:'horizontal'
    pos_hint: {'center_x':0, 'center_y':0.8}

<Container>:
    rows:2
    GridLayout
        size_hint: 1, 0.5
        cols:3

        ItemBox
            padding: [1, 0, 0, 0]
            Label:
                text_size: (420/2, 605/2 )
                valign:'top'
                halign:'left'
                text:'list'
                font_size: '14sp'
        ItemBox
            orientation:'horizontal'
            size_hint: 0.15, 0.5

            Label:
                text:'h'
                font_size: '15sp'
        ItemBox
            size_hint: 0.15, 0.5
            padding: [0, 0, 1, 0]
            Label:
                text:'ch'
                font_size: '15sp'
    GridLayout
        rows: 5
        size_hint: 1, 0.5
        ItemButton:
            text:'ch'
        ItemButton:
            text:'ch'
        ItemButton:
            text:'ch'
        ItemButton:
            text:'ch'

        Button:
            text:'ch'
        Button:
            text:'ch'
        Button:
            text:'ch'
        Button:
            text:'ch'
        Button:
            text:'ch'

        Button:
            text:'ch'
